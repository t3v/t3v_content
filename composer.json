{
  "name": "t3v/t3v_content",
  "type": "typo3-cms-extension",
  "description": "The content extension of TYPO3Voila.",
  "authors": [
    {
      "name": "Maik Kempe",
      "email": "mkempe@bitaculous.com",
      "role": "Developer"
    },
    {
      "name": "Jochen Greiner",
      "email": "greiner@monobloc.de",
      "role": "Developer"
    }
  ],
  "keywords": [
    "CMS",
    "Content",
    "Extension",
    "T3v",
    "TYPO3",
    "TYPO3voila",
    "UH LÁLÁ"
  ],
  "homepage": "https://t3v.github.io/t3v_content/",
  "support": {
    "issues": "https://github.com/t3v/t3v_content/issues"
  },
  "license": "MIT",
  "config": {
    "bin-dir": ".build/bin",
    "vendor-dir": ".build/vendor"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "replace": {
    "typo3-ter/t3v_content": "self.version"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.typo3.org"
    }
  ],
  "require": {
    "php": ">= 7.2 < 7.4",
    "typo3/cms-core": "^9.5 || dev-master",
    "typo3/cms-fluid-styled-content": "^9.5 || dev-master",
    "gridelementsteam/gridelements": "^9.3 || dev-9-0",
    "fluidtypo3/vhs": "^5.2",
    "t3v/t3v_core": "*"
  },
  "require-dev": {
    "mikey179/vfsstream": "^1.6",
    "namelesscoder/typo3-repository-client": "^2.0",
    "nimut/testing-framework": "^4.1",
    "phpunit/phpunit": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "T3v\\T3vContent\\": "Classes"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "T3v\\T3vContent\\Tests\\": "Tests"
    }
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "extension-key": "t3v_content",
      "web-dir": ".build/web"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "mkdir -p .build/web/typo3conf/ext/",
      "[ -L .build/web/typo3conf/ext/t3v_content ] || ln -snvf ../../../../. .build/web/typo3conf/ext/t3v_content"
    ]
  }
}
